<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import java.net.URL?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.shape.Circle?>

<StackPane id="itemPane"
           prefHeight="50.0"
           prefWidth="400.0"
           xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@styles/sgeneral.css" />
        <URL value="@/mx/inbo/gui/styles/sinvitedfrienditem.css"/>
    </stylesheets>
    
    <Circle fx:id="languageIcon"
            radius="20.0"
            StackPane.alignment="CENTER_LEFT" >
    </Circle>
    
    <Label fx:id="languageLabel"
           StackPane.alignment="CENTER" >
        <font>
            <Font name="Segoe UI Semilight" size="20"/>
        </font>
    </Label>
    
    
</StackPane>
